<Window x:Class="lab2.Asterism_Edit_Window"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:lab2"
        mc:Ignorable="d"
        Title="Asterism_Edit_Window" Height="300" Width="300">
    <Grid>
        <TextBox x:Name="EtextBox" HorizontalAlignment="Left" Height="23" Margin="24,29,0,0" Text="{Binding Path=Name, Mode=TwoWay}" VerticalAlignment="Top" Width="240" />
        <TextBox x:Name="EtextBox_Copy1" HorizontalAlignment="Left" Height="23" Margin="24,85,0,0" Text="{Binding Path=Position, Mode=TwoWay}" VerticalAlignment="Top" Width="240"/>
        <TextBlock x:Name="EtextBlock" HorizontalAlignment="Left" Margin="24,72,0,0" TextWrapping="Wrap" Text="Position" VerticalAlignment="Top"/>
        <TextBlock x:Name="EtextBlock_Copy1" HorizontalAlignment="Left" Margin="24,13,0,0" TextWrapping="Wrap" Text="Name" VerticalAlignment="Top"/>
        <Button x:Name="Ebutton" Content="Save" HorizontalAlignment="Left" Margin="102,190,0,0" VerticalAlignment="Top" Width="102" Height="70" Click="Save_Click"/>
    </Grid>
</Window>
